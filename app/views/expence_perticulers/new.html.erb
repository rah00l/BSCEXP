function cursormovment(id)
{
    
    var tot = document.getElementById('expence_perticuler_total');
    var exp_sec = document.getElementById('expence_perticuler1_expence_attributes_0_expence_value');
    if(event.keyCode==13)
    {
      var sub_button=document.getElementById(id);
      if( sub_button!=null)
